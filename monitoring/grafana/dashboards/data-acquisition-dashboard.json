{
  "dashboard": {
    "id": null,
    "title": "Data Acquisition Service Monitoring",
    "tags": ["crypto", "data-acquisition", "microservices"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"data-acquisition-service\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"data-acquisition-service\"}[5m])",
            "legendFormat": "{{method}} {{uri}}"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"data-acquisition-service\"}[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "yAxes": [
          {"label": "Seconds", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 0}
      },
      {
        "id": 4,
        "title": "WebSocket Connection Status",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket_connections_active{service=\"data-acquisition\"}",
            "legendFormat": "{{exchange}} Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Crypto Prices Collected (Last 1h)",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(crypto_prices_collected_total{job=\"data-acquisition-service\"}[1h])",
            "legendFormat": "{{symbol}} - {{exchange}}"
          }
        ],
        "yAxes": [
          {"label": "Price Updates", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 8}
      },
      {
        "id": 6,
        "title": "Kafka Messages Published",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(kafka_producer_record_send_total{job=\"data-acquisition-service\"}[5m])",
            "legendFormat": "{{topic}}"
          }
        ],
        "yAxes": [
          {"label": "Messages/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 8}
      },
      {
        "id": 7,
        "title": "Database Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "hikaricp_connections_active{job=\"data-acquisition-service\"}",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "hikaricp_connections_idle{job=\"data-acquisition-service\"}",
            "legendFormat": "Idle Connections"
          }
        ],
        "yAxes": [
          {"label": "Connections", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Redis Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_cache_hits_total{job=\"data-acquisition-service\"}[5m]) / (rate(redis_cache_hits_total{job=\"data-acquisition-service\"}[5m]) + rate(redis_cache_misses_total{job=\"data-acquisition-service\"}[5m])) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100}
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 9,
        "title": "JVM Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"data-acquisition-service\", area=\"heap\"}",
            "legendFormat": "Heap Used"
          },
          {
            "expr": "jvm_memory_max_bytes{job=\"data-acquisition-service\", area=\"heap\"}",
            "legendFormat": "Heap Max"
          }
        ],
        "yAxes": [
          {"label": "Bytes", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 10,
        "title": "Error Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"data-acquisition-service\", status=~\"4..|5..\"}[5m])",
            "legendFormat": "{{method}} {{uri}} - {{status}}"
          }
        ],
        "yAxes": [
          {"label": "Errors/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 11,
        "title": "Exchange API Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(exchange_api_request_duration_seconds_bucket{job=\"data-acquisition-service\"}[5m]))",
            "legendFormat": "{{exchange}} - 95th percentile"
          }
        ],
        "yAxes": [
          {"label": "Seconds", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "exchange",
          "type": "query",
          "query": "label_values(crypto_prices_collected_total, exchange)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "symbol",
          "type": "query",
          "query": "label_values(crypto_prices_collected_total{exchange=~\"$exchange\"}, symbol)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    }
  }
}
